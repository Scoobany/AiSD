#include <iostream>
using namespace std;

int main ()
{
  int x;
  cout << "WprowadÅº wartosc x: ";
  cin >> x;
  int *tab = new int[x + 1];

  tab[0] = 0;
  tab[1] = 1;
  for (int i = 2; i < x + 1; i++)
    {
      tab[i] = tab[i - 1] + tab[i - 2];
    }

  cout << "Element ciagu dla " << x << " wynosi: " << tab[x];

  delete[]tab;
  return 0;
}
