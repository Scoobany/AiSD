#include <iostream>
using namespace std;

int main()
{
	int n;
	cout << "podaj wartosc n ";
	cin >> n;
	int *tab = new int[n + 1];

	tab[0] = 0;
	tab[1] = 1;
	for (int i = 2; i < n+1; i++) {
		tab[i] = tab[i - 1] + tab[i - 2];
	}
	
	cout << "Wartosc funkcji Fibonacciego dla " << n << " wynosi: " << tab[n];

	delete[]tab;
	return 0;
}
